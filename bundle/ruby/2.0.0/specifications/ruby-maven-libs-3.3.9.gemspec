# -*- encoding: utf-8 -*-

Gem::Specification.new do |s|
  s.name = "ruby-maven-libs"
  s.version = "3.3.9"

  s.required_rubygems_version = Gem::Requirement.new(">= 0") if s.respond_to? :required_rubygems_version=
  s.authors = ["Christian Meier"]
  s.date = "2016-04-22"
  s.description = "maven distribution as gem - no ruby executables !"
  s.email = ["m.kristian@web.de"]
  s.extra_rdoc_files = ["maven-home/README.txt", "maven-home/NOTICE", "maven-home/LICENSE", "README.md"]
  s.files = ["maven-home/README.txt", "maven-home/NOTICE", "maven-home/LICENSE", "README.md"]
  s.homepage = "https://github.com/takari/ruby-maven-libs"
  s.licenses = ["APL"]
  s.require_paths = ["lib"]
  s.rubygems_version = "2.0.14.1"
  s.summary = "maven distribution as gem"
end
